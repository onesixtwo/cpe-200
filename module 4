//The Program will push 5 items on the stack, perform 2 pop operation before displaying the items from the stack.

#include <iostream>
#include <stack> // Include the stack library
using namespace std;

void show(stack<int> stk) {
    stack<int> s = stk;
    while (!s.empty()) {
        cout << " " << s.top(); // Change from front() to top()
        s.pop();
    }
}

int main() {
    stack<int> mystack; // Create an empty stack of integers

    // Push 5 items onto the stack
    mystack.push(10);
    mystack.push(20);
    mystack.push(30);
    mystack.push(40);
    mystack.push(50);

    show(mystack);
    cout << "\nStack size: " << mystack.size()<<"\n";
    cout << "\nPopping 2 stacks\n" ;
    // Perform 2 pop operations
    mystack.pop();
    mystack.pop();

    cout << "\nStack size: " << mystack.size();
    cout << "\nStack top element: " << mystack.top(); // Change from front() to top()

    cout << "\nElements of stack:"; // Display a message

    // Call the show function to display the remaining elements of the stack
    show(mystack);

    return 0;
}
